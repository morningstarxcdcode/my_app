{
  "name": "my_app",
  "version": "0.1.0",
  "private": true,
  "author": "Poushmita <https://github.com/Poushmita>",
  "contributors": [
    {
      "name": "morningstarxcdcode",
      "url": "https://github.com/morningstarxcdcode"
    }
  ],
  "scripts": {
    "setup": "npm ci",
    "dev:web": "cross-env NEXT_TURBOPACK=false next dev --webpack",
    "dev:turbopack": "next dev",
    "dev": "npm run dev:web",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:ci": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
      "ci": "npm ci && npm run validate:config && npm run typecheck && npm run lint:ci && npm run test && npm run build",
      "prebuild": "npm run validate:config",
    "test:coverage": "jest --coverage",
    "e2e": "npx playwright test",
    "export": "next build",
    "build:export": "next build",
    "deploy:gh-pages": "npm run build:export && npx gh-pages -d out",
    "validate:config": "node ./scripts/validate-next-config.js",
    "predev": "npm run validate:config",
    "predev:web": "npm run validate:config",
    "predev:turbopack": "npm run validate:config"
    ,
    "check:all": "npm run validate:config && npm run typecheck && npm run lint:ci && npm run test && npm run build",
    "start:dev": "npm run setup && npm run check:all && npm run dev",
    "start:prod": "npm run check:all && npm run start"
  },
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/morningstarxcdcode/my_app"
  },
  "dependencies": {
    "next": "16.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-plugin-react-compiler": "1.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.0.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5",
    "gh-pages": "^5.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "prepare": "husky install"
}
